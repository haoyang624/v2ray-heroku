{
  "name": "V2ray",
  "description": "deploy V2ray to Heroku.",
  "keywords": ["V2ray"],
  "env": {
    "VER": {
       "description": "输入v2ray Core 版本号（latest表示最新版本）",
       "value": "latest"
     }, 
    "UUID": {
       "description": "Vmess协议默认UUID，请输入自己的UUID",
       "value": "a2412f3d-aa4d-4c1e-8185-6edf8d9bd937"
     },
    "V2RAY_RAY_BUFFER_SIZE": {
       "description": "每个连接的缓存大小，单位MB",
       "value": "64"
     }
  },
  "website": "https://github.com/haoyang624/v2ray-heroku",
  "repository": "https://github.com/haoyang624/v2ray-heroku",
  "stack": "container"
 }
